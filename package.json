{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend setup with NodeJS, Express, MongoDB, socket.",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "copy-schemas": "cpx src/**/*.gql dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node dist/index.js",
    "cron:dev": "nodemon --exec babel-node src/service.cron.js",
    "cron:start": "node dist/service.cron.js",
    "build": "mkdir -p dist && npm run copy-schemas && babel src -s -d dist",
    "dev": "nodemon --exec babel-node src/index.js"
  },
